{
    "compilerOptions": {
        "target": "es5",
        "lib": ["dom", "dom.iterable", "esnext"],
        "allowJs": true,
        "skipLibCheck": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true,
        "esModuleInterop": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "jsx": "preserve",
        "baseUrl": ".",
        "paths": {
            "@components/*": ["src/modules/core/components/*"],
            "@components": ["src/modules/core/components"],
            "@public/*": ["public/*"],
            "@public": ["public"],
            "@definitions/*": ["src/definitions/*"],
            "@definitions": ["src/definitions"],
            "@interfaces/*": ["src/interfaces/*"],
            "@interfaces": ["src/interfaces"],
            "@styles/*": ["src/styles/*"],
            "@styles": ["src/styles"],
            "@components/icons/*": ["src/modules/core/components/icons/*"],
            "@components/icons": ["src/modules/core/components/icons"],
            "@redux/*": ["src/redux/*"],
            "@redux": ["src/redux"],
            "@test/*": ["test/*"],
            "@test": ["test"],
            "@core/*": ["src/modules/core/*"],
            "@core": ["src/modules/core"],
            "@user/*": ["src/modules/user/*"],
            "@user": ["src/modules/user"],
            "@layouts/*": ["src/modules/layouts/*"],
            "@layouts": ["src/modules/layouts"]
        }
    },
    "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
    "exclude": ["node_modules", "test/*", "cypress/*"]
}
